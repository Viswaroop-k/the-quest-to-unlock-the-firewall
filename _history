 \n    . . . . . . . . . . . . . . . . \n    . . . . . . . . . . . . . . . . \n`\n\n// --- Initialization ---\n\nscene.setBackgroundColor(9) // Light blue background (The Cloud)\ninfo.setLife(3)\ninfo.setScore(0)\n\n// Intro Message\ngame.showLongText(\"Welcome, Cyber Guardian!\\n\\nThe network is under attack by Viruses.\", DialogLayout.Center)\ngame.showLongText(\"Mission:\\n1. Avoid the Red Viruses.\\n2. Talk to 3 Mentors to get Encryption Keys.\\n3. Unlock the Firewall to save the system.\", DialogLayout.Center)\n\n// Setup Hero\nhero = sprites.create(heroImg, SpriteKind.Player)\ncontroller.moveSprite(hero, 100, 100)\nhero.setStayInScreen(true)\nscene.cameraFollowSprite(hero)\n\n// Setup NPCs (Spread them out)\n// NPC 1: Top Left area\nlet npc1 = sprites.create(npcGreenImg, SpriteKind.Food)\nnpc1.setPosition(30, 30)\n// NPC 2: Top Right area\nlet npc2 = sprites.create(npcPinkImg, SpriteKind.Food)\nnpc2.setPosition(130, 30)\n// NPC 3: Bottom Left area\nlet npc3 = sprites.create(npcOrangeImg, SpriteKind.Food)\nnpc3.setPosition(30, 100)\n\n// Setup Firewall (Exit) - Bottom Right\nfirewall = sprites.create(firewallImg, SpriteKind.Projectile) // Using Projectile so we can define specific overlaps\nfirewall.setPosition(140, 100)\nfirewall.setImmovable(true)\n\n// Create Enemies (Viruses)\nfor (let i = 0; i < 4; i++) {\n    let virus = sprites.create(virusImg, SpriteKind.Enemy)\n    // Random position away from player\n    virus.setPosition(randint(50, 150), randint(50, 110))\n    // Move randomly\n    virus.setVelocity(randint(-30, 30), randint(-30, 30))\n    virus.setBounceOnWall(true)\n}\n\n// --- Game Logic Functions ---\n\nfunction spawnKeyEffect(target: Sprite) {\n    keyCount += 1\n    music.baDing.play()\n    target.startEffect(effects.starField, 1000)\n    target.sayText(\"Key Acquired!\", 2000, true)\n\n    // Check if we have all keys\n    if (keyCount >= totalKeysNeeded) {\n        game.splash(\"All Keys Collected!\", \"Go to the Firewall!\")\n        firewall.startEffect(effects.fountain, 2000)\n    }\n}\n\n// --- Event Handlers ---\n\n// 1. Collision with Virus (Enemy)\nsprites.onOverlap(SpriteKind.Player, SpriteKind.Enemy, function (player, enemy) {\n    info.changeLifeBy(-1)\n    music.powerDown.play()\n    player.startEffect(effects.fire, 500)\n    // Push player away safely\n    if (player.x < enemy.x) {\n        player.x -= 20\n    } else {\n        player.x += 20\n    }\n    pause(500) // Invulnerability frames\n})\n\n// 2. Interaction with NPC 1 (Passwords)\nsprites.onOverlap(SpriteKind.Player, SpriteKind.Food, function (player, npc) {\n    // We use SpriteKind.Food for NPCs just to group them\n\n    // Stop player movement during dialog\n    controller.moveSprite(hero, 0, 0)\n\n    if (npc == npc1) {\n        if (!npc1Solved) {\n            game.showLongText(\"Mentor Green: To enter the secure zone, you need a strong password.\", DialogLayout.Bottom)\n            let answer = game.ask(\"Is '123456' a good password?\", \"Yes\", \"No\")\n            if (answer == false) { // No is correct\n                game.showLongText(\"Correct! Use a mix of letters, numbers, and symbols.\", DialogLayout.Bottom)\n                npc1Solved = true\n                spawnKeyEffect(npc)\n                npc1.setImage(keyImg) // Turn into key representation or just change looks\n            } else {\n                game.showLongText(\"Unsafe! Hackers can guess that instantly. Try again.\", DialogLayout.Bottom)\n                info.changeLifeBy(-1)\n            }\n        } else {\n            npc.sayText(\"Stay secure!\", 500, true)\n        }\n    }\n\n    else if (npc == npc2) {\n        if (!npc2Solved) {\n            game.showLongText(\"Mentor Pink: Watch out for Phishing scams.\", DialogLayout.Bottom)\n            let answer = game.ask(\"You win a FREE iPhone! Click the link?\", \"No way!\", \"Click it!\")\n            if (answer == true) { // \"No way!\" is the first option (true)\n                game.showLongText(\"Smart! If it sounds too good to be true, it is.\", DialogLayout.Bottom)\n                npc2Solved = true\n                spawnKeyEffect(npc)\n                npc2.setImage(keyImg)\n            } else {\n                game.showLongText(\"Oh no! You downloaded a virus. Never click suspicious links.\", DialogLayout.Bottom)\n                info.changeLifeBy(-1)\n            }\n        } else {\n            npc.sayText(\"Don't click links!\", 500, true)\n        }\n    }\n\n    else if (npc == npc3) {\n        if (!npc3Solved) {\n            game.showLongText(\"Mentor Orange: Being a good citizen means being kind.\", DialogLayout.Bottom)\n            let answer = game.ask(\"Someone is being bullied online. Do you:\", \"Laugh\", \"Tell an Adult\")\n            if (answer == false) { // \"Tell an Adult\" is second (false)\n                game.showLongText(\"Excellent. Always report bullying.\", DialogLayout.Bottom)\n                npc3Solved = true\n                spawnKeyEffect(npc)\n                npc3.setImage(keyImg)\n            } else {\n                game.showLongText(\"That's not nice. We should help others.\", DialogLayout.Bottom)\n                info.changeLifeBy(-1)\n            }\n        } else {\n            npc.sayText(\"Be kind online!\", 500, true)\n        }\n    }\n\n    // Restore movement + clear overlap slightly to prevent double triggering immediately\n    player.x += 5\n    controller.moveSprite(hero, 100, 100)\n})\n\n// 3. Reaching the Firewall (Exit)\nsprites.onOverlap(SpriteKind.Player, SpriteKind.Projectile, function (player, exit) {\n    if (exit == firewall) {\n        if (keyCount >= totalKeysNeeded) {\n            game.over(true, effects.confetti)\n        } else {\n            exit.sayText(\"Need \" + (totalKeysNeeded - keyCount) + \" more keys!\", 500, true)\n            // Push back slightly\n            player.x -= 10\n            player.y -= 10\n        }\n    }\n})"]]}]}]},{"timestamp":1764886078735,"editorVersion":"2.0.62"